<!DOCTYPE html>
<html>
<head>
  <title>Login</title>
  <link rel="stylesheet" href="../style.css">
</head>
<body>
  <h1>Login</h1>

  <%= form_tag(sessions_path, class: "login") do %>
    <div class="container_input" >
      <%= label_tag :email %>
      <%= email_field_tag :email %>
    </div>
    <div class="container_input" >
      <%= label_tag :password %>
      <%= password_field_tag :password %>
    </div>
    <%= submit_tag "Login" %>
  <% end %>
</body>
</html>
